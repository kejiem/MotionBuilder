test = []
def Test1(pList):
    pList.append("test")
    print pList
    return pList
Test1(test)
print test

print "---"

test = ""
def Test2(pList):
    pList = ["test"]
    print pList
    return pList
Test2(test)
print test

print "---"

test = ""
def Test3(pList):
    pList = "test"
    print pList
    return pList
Test3(test)
print test
